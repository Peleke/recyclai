    "url": "https://github.com/Peleke/recyclai/issues"
  },
  "homepage": "https://github.com/Peleke/recyclai#readme",
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.8",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "next": "^16.0.3",
    "postcss": "^8.5.6",
